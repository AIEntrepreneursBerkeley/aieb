{
    "editor.formatOnSave": true,
    "[javascript][javascriptreact][typescript][typescriptreact]": {
        "editor.formatOnSave": false
    },
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "emmet.includeLanguages": {
        "typescript": "typescriptreact"
    },
    "files.associations": {
        "*.css": "tailwindcss"
    },
    "editor.quickSuggestions": {
        "strings": "on"
    }
}